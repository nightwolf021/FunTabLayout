<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tompee.funtablayoutsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tompee.funtablayoutsample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".simpletablayout.SimpleTabLayoutActivity"
            android:label="Simple Tab Layout" />
        <activity
            android:name=".bubbletablayout.BubbleTabLayoutActivity"
            android:label="Bubble Tab Layout" />
        <activity
            android:name=".poptablayout.PopTabLayoutActivity"
            android:label="Pop Tab Layout" />
    </application>

</manifest>